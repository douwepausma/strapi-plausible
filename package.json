{
  "name": "strapi-plausible",
  "description": "Show Plausible analytics within Strapi.",
  "version": "2.0.1",
  "keywords": [
    "strapi",
    "plausible",
    "analytics",
    "plugin"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json"
  },
  "dependencies": {
    "react-intl": "^7.1.11",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.19.0",
    "@strapi/typescript-utils": "^5.19.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@strapi/design-system": "^2.0.0-rc.29",
    "@strapi/icons": "^2.0.0-rc.29",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "plausible",
    "displayName": "Plausible",
    "description": "Show Plausible analytics within Strapi."
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/douwepausma/strapi-plausible.git"
  },
  "bugs": {
    "url": "https://github.com/douwepausma/strapi-plausible/issues"
  },
  "homepage": "https://github.com/douwepausma/strapi-plausible#readme",
  "author": "douwepausma <mail@douwepausma.nl>"
}
